\n          <div class=\"title\">\n            <h1 class=\"m-0\">\n              {{ t('authGroups.assignEdit.assignTitle') }}\n            </h1>\n          </div>\n        </header>\n      </div>\n\n      <form onsubmit=\"return false;\">\n        <SelectPrincipal\n          :retain-selection=\"true\"\n          class=\"mb-20\"\n          :show-my-group-types=\"['group']\"\n          :search-group-types=\"'group'\"\n          @add=\"setPrincipal\"\n        />\n\n        <GlobalRoleBindings\n          ref=\"grb\"\n          :group-principal-id=\"principalId\"\n          :mode=\"mode\"\n          :assign-only=\"true\"\n          @canLogIn=\"canLogIn = $event\"\n          @hasChanges=\"rolesChanged = $event\"\n        />\n\n        <FooterComponent\n          :mode=\"editMode\"\n          :errors=\"errors\"\n          :disable-save=\"!canSave\"\n          @save=\"save\"\n          @done=\"cancel\"\n        />\n      </form>\n    </div>\n  </div>\n</template>\n\n<style lang=\"scss\" scoped>\n  .masthead {\n    padding-bottom: 10px;\n    border-bottom: 1px solid var(--border);\n    margin-bottom: 10px;\n  }\n  HEADER {\n    margin: 0;\n  }\n  .actions {\n    display: flex;\n    justify-content: flex-end;\n    align-items:center;\n\n    .btn {\n      margin-left: 10px;\n    }\n  }\n</style>\n"],"mappings":";;;;;AACA,OAAAA,eAAA;AACA,OAAAC,eAAA;AACA,OAAAC,kBAAA;AACA,SAAAC,MAAA;AACA,SAAAC,KAAA,EAAAC,KAAA;AACA,SAAAC,sBAAA;AACA,SAAAC,IAAA;AACA,SAAAC,aAAA;AAEA;EACAC,UAAA;IACAR,eAAA,EAAAA,eAAA;IACAD,eAAA,EAAAA,eAAA;IACAE,kBAAA,EAAAA;EACA;EACAQ,IAAA,WAAAA,KAAA;IACA;MACAC,MAAA;MACAC,WAAA;MACAC,QAAA;MACAC,YAAA;MACAC,QAAA,EAAAV;IACA;EACA;EACAW,QAAA;IACAC,IAAA,WAAAA,KAAA;MACA,aAAAL,WAAA,GAAAR,KAAA,QAAAc,MAAA,CAAAC,KAAA,CAAAF,IAAA,IAAAb,KAAA;IACA;IACAgB,OAAA,WAAAA,QAAA;MACA,YAAAN,YAAA,SAAAD,QAAA;IACA;EACA;EACAQ,OAAA;IACAC,YAAA,WAAAA,aAAAC,EAAA;MACA,KAAAX,WAAA,GAAAW,EAAA;MAEA;IACA;IACAC,MAAA,WAAAA,OAAA;MAAA,IAAAC,KAAA;MAAA,OAAAC,iBAAA,eAAAC,mBAAA,CAAAC,IAAA,UAAAC,QAAA;QAAA,OAAAF,mBAAA,