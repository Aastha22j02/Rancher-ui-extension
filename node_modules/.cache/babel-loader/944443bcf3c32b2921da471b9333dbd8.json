h,\n        ...this.stylesPropToObj,\n      };\n    }\n  },\n  mounted() {\n    document.addEventListener('keydown', this.handleEscapeKey);\n  },\n  beforeDestroy() {\n    document.removeEventListener('keydown', this.handleEscapeKey);\n  },\n  methods: {\n    handleClickOutside(event: MouseEvent) {\n      if (\n        this.clickToClose &&\n        this.$refs.modalRef &&\n        !(this.$refs.modalRef as HTMLElement).contains(event.target as Node)\n      ) {\n        this.$emit('close');\n      }\n    },\n    handleEscapeKey(event: KeyboardEvent) {\n      if (this.clickToClose && event.key === 'Escape') {\n        this.$emit('close');\n      }\n    }\n  }\n});\n"],"mappings":";;;;;;;;;;;;;;;;AACA,SAASA,eAAe,QAAQ,KAAK;AAErC,eAAeA,eAAe,CAAC;EAC7BC,IAAI,EAAU,UAAU;EACxBC,YAAY,EAAE,KAAK;EACnBC,KAAK,EAAS;IACZ;;;;IAIAC,YAAY,EAAE;MACZC,IAAI,EAAKC,OAAO;MAChBC,OAAO,EAAE;KACV;IACD;;;;;IAKAC,KAAK,EAAE;MACLH,IAAI,EAAK,CAACI,MAAM,EAAEC,MAAM,CAAC;MACzBH,OAAO,EAAE,GAAG;MACZI,SAAS,WAATA,SAASA,CAACC,KAAK;QACb,IAAI,OAAOA,KAAK,KAAK,QAAQ,EAAE;UAC7B,OAAOA,KAAK,GAAG,CAAC;;QAGlB,IAAI,OAAOA,KAAK,KAAK,QAAQ,EAAE;UAC7B,OAAO,sCAAsC,CAACC,IAAI,CAACD,KAAK,CAAC;;QAG3D,OAAO,KAAK;MACd;KACD;IACD;;;IAGAE,WAAW,EAAE;MACXT,IAAI,EAAKK,MAAM;MACfH,OAAO,EAAE;KACV;IACD;;;IAGAQ,MAAM,EAAE;MACNV,IAAI,EAAKK,MAAM;MACfH,OAAO,EAAE;KACV;IACD;;;IAGAN,IAAI,EAAE;MACJI,IAAI,EAAKK,MAAM;MACfH,OAAO,EAAE;;GAEZ;EACDS,QAAQ,EAAE;IACRC,UAAU,WAAVA,UAAUA,CAAA;MACR,IAAMC,GAAG,GAAG,OAAQ,IAAI,CAACV,KAAM,KAAK,QAAQ,GAAG,IAAI,GAAG,EAAE;MAExD,UAAAW,MAAA,CAAW,IAAI,CAACX,KAAM,EAAAW,MAAA,CAAID,GAAI;IAChC,CAAC;IACDE,eAAe,WAAfA,eAAeA,CAAA;MACb,OAAO,IAAI,CAACL,MAAM,CAACM,KAAK,CAAC,GAAG,CAAC,CAC1BC,GAAG,CAAC,UAACC,IAAI;QAAA,OAAKA,IAAI,CAACC,IAAI,EAAE,CAACH,KAAK,CAAC,GAAG,CAAC;MAAA,EAAC,CACrCI,MAAM,CAAC,UAACC,KAAK,EAAAC,IAAA,EAAgB;QAAA,IAAAC,KAAA,GAAAC,cAAA,CAAAF,IAAA;UAAbG,GAAG,GAAAF,KAAA;UAAEG,GAAG,GAAAH,KAAA;QACvB,OAAAI,aAAA,CAAAA,aAAA,KACKN,KAAK,OAAAO,eAAA,KACPH,GAAG,EAAGC,GAAG;MAEd,CAAC,EAAE,EAAG,CAAC;IACX,CAAC;IACDG,WAAW,WAAXA,WAAWA,CAAA;M