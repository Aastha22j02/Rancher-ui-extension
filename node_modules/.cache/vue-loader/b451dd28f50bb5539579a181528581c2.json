     if (show) {\n        this.showModal = true;\n      } else {\n        this.showModal = false;\n      }\n    },\n    async submit(buttonCb) {\n      try {\n        await this.$refs.changePassword.save();\n        this.show(false);\n        buttonCb(true);\n      } catch (err) {\n        buttonCb(false);\n      }\n    }\n  },\n};\n</script>\n\n<template>\n  <app-modal\n    v-if=\"showModal\"\n    custom-class=\"change-password-modal\"\n    name=\"password-modal\"\n    :width=\"500\"\n    :height=\"465\"\n    @close=\"show(false)\"\n  >\n    <Card\n      class=\"prompt-password\"\n      :show-highlight-border=\"false\"\n    >\n      <h4\n        slot=\"title\"\n        class=\"text-default-text\"\n      >\n        {{ t(\"changePassword.title\") }}\n      </h4>\n      <div slot=\"body\">\n        <form @submit.prevent>\n          <ChangePassword\n            ref=\"changePassword\"\n            @valid=\"valid = $event\"\n          />\n        </form>\n      </div>\n\n      <template #actions>\n        <!-- type reset is required by lastpass -->\n        <button\n          class=\"btn role-secondary\"\n          type=\"reset\"\n          @click=\"show(false)\"\n        >\n          {{ t(\"changePassword.cancel\") }}\n        </button>\n        <AsyncButton\n          type=\"submit\"\n          mode=\"apply\"\n          class=\"btn bg-error ml-10\"\n          :disabled=\"!valid\"\n          value=\"LOGIN\"\n          @click=\"submit\"\n        />\n      </template>\n    </Card>\n  </app-modal>\n</template>\n\n<style lang=\"scss\" scoped>\n  .prompt-password {\n    ::v-deep .card-wrap {\n      display: flex;\n      flex-direction: column;\n\n      .card-body {\n        flex: 1;\n        justify-content: start;\n        & > div {\n          flex: 1;\n          display: flex;\n        }\n      }\n\n      .card-actions {\n        display: flex;\n        justify-content: flex-end;\n        width: 100%;\n      }\n    }\n  }\n\n  .prompt-password {\n    flex: 1;\n    display: flex;\n    form {\n      flex: 1;\n    }\n  }\n</style>\n"]}]}